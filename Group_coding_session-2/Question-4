#!/bin/bash

# Prompt the user to enter two numbers
echo "Please enter the first number:"
read num1
echo "Please enter the second number:"
read num2

# Compute the product of the two numbers
product=$((num1 * num2))

# Print the result
echo "The product of $num1 and $num2 is $product"

#!/bin/bash

# Read each line of the input file and create a folder with that name
while read folder_name; do
  mkdir "$folder_name"
done < folders.txt

